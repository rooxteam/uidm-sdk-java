<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <suppress>
        <notes><![CDATA[
        This suppresses CVEs with LOW severity.
        ]]></notes>
        <cvssBelow>4</cvssBelow>
    </suppress>
    <suppress>
        <notes><![CDATA[
        This suppresses false positives identified on RooX Solutions libraries.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.rooxteam\..*$</packageUrl>
        <cvssBelow>10</cvssBelow>
    </suppress>
</suppressions>
